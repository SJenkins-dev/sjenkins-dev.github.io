---
import client from '../../tina/__generated__/client';
import HomePage from "../../tina/pages/HomePage.tsx";
import PageComponent from '../components/PageComponent.astro';
import SEOComponent from '../components/SEOPageComponent.astro';
import PortfolioLoop from '../components/partials/Portfolio/PortfolioLoop.astro';

const data = await client.queries.page({relativePath: "home.mdx"});
const canonicalURL = new URL(Astro.url.pathname, Astro.site); 

---
		<SEOComponent data={data} canonicalURL={canonicalURL}/>
		<PageComponent>
		<HomePage {...data} client:tina />
		<PortfolioLoop/>
		</PageComponent>

		
		
	  
	  

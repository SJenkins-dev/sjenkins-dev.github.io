---
const { data, canonicalURL } = Astro.props;
import { AstroSeo } from '@astrolib/seo';
---

<AstroSeo
		title={data.data.project.title},
		description={data.data.project.description},
		canonical={canonicalURL},
		openGraph={{
		  url: canonicalURL,
		  type: "website",
		  title: data.data.project.title,
		  description: data.data.project.description,
		  site_name: data.data.project.title,
		}}
	  />
---
const { data, canonicalURL } = Astro.props;
import { AstroSeo } from '@astrolib/seo';
const opengraph = {...data.data.project.seo?.openGraph};
const twitter = {...data.data.project.seo?.twitter};
---

<AstroSeo
		title={data.data.project.title},
		description={data.data.project.description},
		canonical={canonicalURL},
		openGraph={{
		  url: canonicalURL,
		  type: opengraph.type,
		  title: data.data.project.title,
		  description: data.data.project.description,
		  site_name: opengraph.siteName,
		  images: opengraph.images,
		}}
		twitter={{
		  handle: twitter.handle,
		  site: twitter.site,
		  cardType: twitter.cardType,
		}}
	  />
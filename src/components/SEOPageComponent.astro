---
const { data, canonicalURL } = Astro.props;
import { AstroSeo } from '@astrolib/seo';
---

<AstroSeo
		title={data.data.page.seotitle},
		canonical={canonicalURL},
		openGraph={{
		  url: canonicalURL,
		  type: "website",
		  title: data.data.page.title,
		  description: data.data.page.description,
		  site_name: data.data.page.title,
		}}
	  />